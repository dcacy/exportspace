applications:
- path: .
  memory: 256M
  instances: 1
  domain: mybluemix.net
  name: <your app name>
  host: <your host name>
  disk_quota: 1024M
  env:
    APP_ID: <your app id>
    APP_SECRET: <your app secret>
    DEBUG: dpc-exportspace-*
    WW_SDK_LOG_LEVEL: info
    OAUTH_SUCCESS_PAGE: /oauthSuccess